<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Baumaschine.BaumaschinenFragment"
    android:background="@color/white">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/archivedBaumaschineRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:itemCount="5"
        tools:listitem="@layout/recyclerview_baumaschinen_item" />

    <TextView
        android:id="@+id/emptyArchivedBaumaschinenRecyclerviewTextView"
        android:layout_width="match_parent"
        android:layout_height="555dp"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:text="@string/emptyArchivedBaumaschinenRecyclerViewRecyclerView"
        android:visibility="gone"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />


</FrameLayout>